{
  "name": "@discourse/ember-instantsearch",
  "version": "1.0.5",
  "description": "A Ember v2 addon which acts as a wrapper for Algolia's InstantSearch library integrated with a Typesense adapter",
  "keywords": [
    "ember-addon",
    "ember",
    "instantsearch",
    "algolia",
    "ui",
    "autocomplete",
    "typesense"
  ],
  "repository": "https://github.com/discourse/ember-instantsearch",
  "license": "MIT",
  "author": "",
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*.js",
    "./addon-main.js": "./addon-main.cjs"
  },
  "files": [
    "addon-main.cjs",
    "declarations",
    "dist"
  ],
  "scripts": {
    "build": "rollup --config",
    "lint": "concurrently 'npm:lint:*(!fix)' --names 'lint:'",
    "lint:fix": "concurrently 'npm:lint:*:fix' --names 'fix:'",
    "lint:hbs": "ember-template-lint . --no-error-on-unmatched-pattern",
    "lint:hbs:fix": "ember-template-lint . --fix --no-error-on-unmatched-pattern",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "prepack": "rollup --config",
    "start": "rollup --config --watch",
    "test": "echo 'A v2 addon does not have tests, run tests in test-app'"
  },
  "dependencies": {
    "@babel/runtime": "^7.24.4",
    "@ember/render-modifiers": "^2.1.0",
    "@embroider/addon-shim": "^1.8.7",
    "algoliasearch": "^4.22.1",
    "decorator-transforms": "^1.2.1",
    "ember-cli-htmlbars": "ember-cli/ember-cli-htmlbars",
    "instantsearch.js": "^4.66.0",
    "typesense-instantsearch-adapter": "^2.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/eslint-parser": "^7.23.3",
    "@babel/runtime": "^7.24.4",
    "@embroider/addon-dev": "^4.1.0",
    "@rollup/plugin-babel": "^6.0.4",
    "babel-plugin-ember-template-compilation": "^2.2.1",
    "concurrently": "^8.2.2",
    "ember-template-lint": "^5.13.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-ember": "^11.12.0",
    "eslint-plugin-n": "^16.4.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.1.1",
    "prettier-plugin-ember-template-tag": "^1.1.0",
    "rollup": "^4.9.1",
    "rollup-plugin-copy": "^3.5.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "version": 2,
    "type": "addon",
    "main": "addon-main.cjs",
    "app-js": {
      "./components/ais-base-widget.js": "./dist/_app_/components/ais-base-widget.js",
      "./components/ais-clear-refinements.js": "./dist/_app_/components/ais-clear-refinements.js",
      "./components/ais-current-refinements.js": "./dist/_app_/components/ais-current-refinements.js",
      "./components/ais-hits-per-page.js": "./dist/_app_/components/ais-hits-per-page.js",
      "./components/ais-hits.js": "./dist/_app_/components/ais-hits.js",
      "./components/ais-infinite-hits.js": "./dist/_app_/components/ais-infinite-hits.js",
      "./components/ais-instant-search.js": "./dist/_app_/components/ais-instant-search.js",
      "./components/ais-menu-select.js": "./dist/_app_/components/ais-menu-select.js",
      "./components/ais-menu.js": "./dist/_app_/components/ais-menu.js",
      "./components/ais-numeric-menu.js": "./dist/_app_/components/ais-numeric-menu.js",
      "./components/ais-pagination.js": "./dist/_app_/components/ais-pagination.js",
      "./components/ais-panel.js": "./dist/_app_/components/ais-panel.js",
      "./components/ais-powered-by.js": "./dist/_app_/components/ais-powered-by.js",
      "./components/ais-range-slider.js": "./dist/_app_/components/ais-range-slider.js",
      "./components/ais-rating-menu.js": "./dist/_app_/components/ais-rating-menu.js",
      "./components/ais-refinement-list.js": "./dist/_app_/components/ais-refinement-list.js",
      "./components/ais-relevant-sort.js": "./dist/_app_/components/ais-relevant-sort.js",
      "./components/ais-search-box.js": "./dist/_app_/components/ais-search-box.js",
      "./components/ais-sort-by.js": "./dist/_app_/components/ais-sort-by.js",
      "./components/ais-stats.js": "./dist/_app_/components/ais-stats.js",
      "./components/index.js": "./dist/_app_/components/index.js",
      "./services/ais-instant-search.js": "./dist/_app_/services/ais-instant-search.js"
    }
  }
}
